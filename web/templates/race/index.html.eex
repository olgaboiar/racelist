<div>
  <h4>My Races</h4>
  <ul clas="collection">
  <%= for race <- @races do %>
    <li class="collection-item">
      <%= race.title %>
      <div class="right">
        <%= link "Edit", to: race_path(@conn, :edit, race)%>
      </div>
    </li>
  <% end %>
</ul>
  <div class="fixed-action-btn">
    <%= link to: race_path(@conn, :new), class: "btn-floating btn-large waves-effect waves-light red"  do %>
      <i class="material-icons">add race</i>
    <% end %>
  </div>
</div>
